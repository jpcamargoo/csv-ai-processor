{
  "name": "app-csv-ia",
  "version": "1.0.0",
  "description": "Aplicação para processar CSV com IA - reescrever, resumir e expandir conteúdo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Build step'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron": "electron electron.js",
    "electron-dev": "NODE_ENV=development electron electron.js",
    "build-win": "electron-builder --win",
    "build-mac": "electron-builder --mac",
    "build-linux": "electron-builder --linux",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "csv",
    "ai",
    "openai",
    "processing",
    "express"
  ],
  "author": "Desenvolvedor",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "electron": "^27.3.11",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "./",
  "build": {
    "appId": "com.seudominio.csv-ia-processor",
    "productName": "CSV + IA Processor",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!dist",
      "!node_modules"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  }
}
